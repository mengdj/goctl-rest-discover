syntax = "v1"

info(
	title: "type title here"
	desc: "type desc here"
	author: "type author here"
	email: "type email here"
	version: "type version here"
)

type (
	Response {
		Code uint32 `json:"code"`
		Msg  string `json:"msg"`
	}
)

@server(
	group: test
	timeout: 30s
)
service test_api {
	@handler Hello
	get /api/v1/app/test/hello () returns (Response)
}